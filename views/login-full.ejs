<body id="login-page-full">
<div id="login-full-wrapper">
<div class="container"> 
<div class="row">
<div class="col-xs-12">
<div id="login-box">
<div id="login-box-holder">
<div class="row">
<div class="col-xs-12">
	<%if(errormessage !==null){%>
		<div class="alert alert-danger" role="alert" style="text-align: center; "><%=errormessage%></div>
	<%}
	else if(successmessage !==null){%>
		<div class="alert alert-success" role="alert" style="text-align: center;"><%=successmessage%></div>
	<%}%>
<header id="login-header">
<div id="login-logo">
	<h4 style="font-size: 100%; color: whitesmoke;">Login</h4>
<!-- <img src="img/logo.png" alt=""/> -->
</div>
</header>
<div id="login-box-inner">
<form role="form" action="/login" method="POST">
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-user"></i></span>
<input class="form-control" type="text" name="email" placeholder="Email address">
</div>
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-key"></i></span>
<input type="password" class="form-control" name="pass" placeholder="Password">
</div>
<div class="row">
<div class="col-xs-12">
<button type="submit" class="primary_btn rounded-0 primary-bg text-white w-100 col-xs-12">Login</button>
</div>
</div>
<!-- <div class="row">
<div class="col-xs-12">
<p class="social-text">Or login with</p>
