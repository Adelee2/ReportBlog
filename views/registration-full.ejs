<body id="login-page-full">
<div id="login-full-wrapper">
<div class="container">
<div class="row">
<div class="col-xs-12">
<div id="login-box">
<div class="row">
<div class="col-xs-12">

<header id="login-header">
<div id="login-logo">
	<h4 style="font-size: 100%; color: whitesmoke; ">Register</h4>
<!-- <img src="img/logo.png" alt=""/> -->
</div>
</header>
<div id="login-box-inner">
	<!-- <% if (errormessage){ 
		for(var i=0;i<errormessage.length;i++){ %>
			<span class="input-group"><%=errormessage[i].msg %></span>
	<%	}
	 } %> -->
	 <% if (errormessage=== false){%>
		<span class="input-group">password not the same!</span>
	<% } %>
<form role="form" action="/register" method="POST" enctype="multipart/form-data">
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-user"></i></span>
<input class="form-control" value="<%=oldinput.name %>" name="names" type="text" placeholder="Full name" required="required">
</div>
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-envelope"></i></span>
<input class="form-control" value="<%=oldinput.email %>" name="email" type="text" placeholder="Email address" required="required">
</div>
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-lock"></i></span>
<input type="password" value="<%=oldinput.password %>" class="form-control" name="pass1" placeholder="Enter password" required="required">
</div>
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-unlock-alt"></i></span>
<input type="password" value="<%=oldinput.password2 %>" class="form-control" name="pass2" placeholder="Re-enter password" required="required">
</div>
<div class="input-group">
<span class="input-group-addon"><i class="fa fa-unlock-alt"></i></span>
<input type="file" class="form-control" name="fileupload" >
</div>
